[MASTER]
ignore = alembic

[MESSAGES CONTROL]
disable=
    missing-docstring,
    too-many-arguments,
    line-too-long,
    missing-module-docstring,
    missing-function-docstring,
    import-error,
    too-few-public-methods,
    use-dict-literal,
    unspecified-encoding,
    pointless-string-statement

[FORMAT]
max-line-length=120